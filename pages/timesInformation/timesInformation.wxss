page {
  position: relative;
  min-height: 100%;
  background: #EBEFF8;
}

.times-container {
  padding: 24rpx;
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
}

.ipt-box {
  position: relative;
  display: flex;
  padding: 0 24rpx;
  justify-content: space-between;
  align-items: center;
  color: #8A8A92;
}

.lab {
  font-size: 30rpx;
}

.van-cell {
  padding: 16px 15px !important;
  border-bottom: 1px solid #1A1E27 !important;
  background: none !important;
}

.ipt {
  padding-left: 16rpx !important;
}

.note {
  margin-top: 64rpx;
  display: flex;
  padding: 0 12rpx;
  font-size: 32rpx;
  color: #686B72;
}

.btn {
  position: absolute;
  display: flex;
  padding: 0 24rpx;
  width: calc(100% - 48rpx);
  height: 88rpx;
  justify-content: center;
  align-items: center;
  bottom: 88rpx;
  border-radius: 64rpx;
  background: #FC3B40;
  color: #FFF;
}

.dialog-info {
  margin: 12rpx 24rpx 64rpx;
  height: 64rpx;
  line-height: 64rpx;
}
