page {
  height: 100%;
  background: #EFEFF4;
}

.entrance {
  margin-top: 24rpx;
}

.list {
  display: flex;
  flex-direction: column;
  background: #fff;
}

.item-box {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 12rpx 24rpx;
  margin-bottom: 36rpx;
  width: 100%;
  height: 550rpx;
  background: #fff;
}

.img-tip {
  flex: 1;
  width: 100%;
}

.title {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 40rpx;
  color: #333;
}
