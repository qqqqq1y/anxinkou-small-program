page {
  position: relative;
  min-height: 100%;
  background: #eeeeee;
}

.ct-container {
  height: 100%;
}

.list {
  width: 100%;
}

.item {
  position: relative;
  display: flex;
  margin-top: 12rpx;
  flex-direction: column;
  padding: 24rpx 0 12rpx;
  width: 100%;
  background: #ffffff;
  color: #818181;
}

.col {
  display: flex;
  padding: 0 30rpx;
  min-height: 48rpx;
  line-height: 48rpx;
  font-size: 24rpx;
}

.lab {
  margin-right: 18rpx;
  /* white-space:nowrap; */
}

.col-btn {
  position: absolute;
  right: 30rpx;
  border-radius: 10rpx;
  background: #FE634D;
  color: #FFF;
  font-size: 24rpx;
}

.col-btn-top {
  padding: 6rpx 36rpx;
  top: 160rpx;
}

.col-btn-bottom {
  padding: 6rpx 56rpx;
  bottom: 12rpx;
}

.review {
  margin-left: 24rpx;
  color: #E65A7D;
}

.contract-box {
  position: absolute;
  width: 100%;
  height: 100%;
  text-align: center;
  top: 0;
  z-index: 100;
}

.mask {
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.5;
}

.img {
  position: fixed;
  width: 60%;
  height: 96%;
  z-index: 101;
  top: 24rpx;
  left: 20%;
}



